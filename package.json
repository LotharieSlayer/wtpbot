{
  "name": "wtpbot",
  "version": "2.3.0",
  "main": "src/main.js",
  "scripts": {
    "build": "tsc && tsc-alias && copyfiles -u 1 \"src/files/**/*\" dist",
    "start": "npm run build && npm run start-prod",
    "start-prod": "node dist/main.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/main.ts",
    "up_cmd": "npm run build && node dist/main.js -l",
    "list": "echo \"Liste des plugins téléchargés :\" && cd src/plugins && ls -l | grep ^d | awk '{print $9}'",
    "update": "chmod u+x scripts/update.sh && scripts/update.sh"
  },
  "dependencies": {
    "@discordjs/rest": "^2.0.0",
    "ansicolor": "^1.1.100",
    "discord-api-types": "^0.37.52",
    "discord.js": "^14.12.1",
    "dotenv": "^16.0.3",
    "enmap": "^5.9.8",
    "fs": "^0.0.1-security",
    "glob": "^7.2.3",
    "i18next": "^21.8.16",
    "mongodb": "^5.1.0",
    "node-cache": "^5.1.2"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
